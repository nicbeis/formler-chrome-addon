{
  "name": "Formler",
  "description": "A Basic Form Filler Extension",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs" ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16formler.png",
      "32": "/images/32formler.png",
      "48": "/images/48formler.png",
      "128": "/images/128formler.png"
    }
  },
  "icons": {
    "16": "/images/16formler.png",
    "32": "/images/32formler.png",
    "48": "/images/48formler.png",
    "128": "/images/128formler.png"
  },
  "options_page": "options.html"
}
